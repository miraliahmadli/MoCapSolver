{
    "train_datadir": "dataset/c3d_npy",
    "train_filenames": "dataset/meta_data.csv",
    
    "val_datadir": "dataset/c3d_npy",
    "var_filenames": "dataset/meta_data.csv",
    
    "test_datadir": "dataset/c3d_npy",
    "test_dir": "dataset/meta_data.csv",
    
    "model_dir": "1/model.pth",

    "train_sum": "1/tensorboard/train",
    "val_sum": "1/tensorboard/val",
    
    "epochs": 100,
    "batch_size": 128,
    "num_markers": 41,
    "num_joints": 31,

    "model":{
        "available": ["baseline", "least_square", "modified_baseline", "gnn"],
        "used": "baseline",
        "baseline": {
            "hidden_size":2048,
            "num_layers": 5,
            "use_svd": false
        }
    },

    "use_gpu":true,

    "optimizer":{
        "available": ["SGD", "Adam"],
        "used": "SGD",
        "SGD": {
            "lr": 0.001
        },
        "Adam": {
            "lr": 0.001
        }
    }
}
